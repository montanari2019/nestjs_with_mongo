generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model depositos {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  numero_pa      Int
  valor_deposito Float
  data_movimento DateTime
  modalidade     String
}
